                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12" style="padding-left: 0;">
                                    <div class="blk blk-txt">
                                        <img src="utilities/captcha.png" alt="captcha" id="captcha">
                                        <a href="javascript:void(0);" class="capthca-btn" id="captcha-refresh" title="refresh the antispam code"><i class="fa fa-refresh"></i></a>
                                    </div>
                                </div>
                                
                                <div class="clear"></div>
                                <input type="checkbox" name="accetto" id="accetto" class="styled" /> <p>I have read and agree to the Privacy Policy * (<a href="privacy.html" class="privacy" target="_blank" title="Info">Info</a>)</p>
                                <div class="clear"></div>
                                <input type="checkbox" name="iscrivimi_newsletter" id="iscrivimi_newsletter" class="styled" /> <p>Subscribe to our newsletter</p>
                                <div class="clear"><br/></div>
                                
                                <input type="text" id="fred" name="fred" style="visibility: hidden;"/>
                                <a href="javascript:void(0);" onClick="javascript:contatti_form();" class="btn float-left" title="send">send</a>
                            </form>
                            <script language="javascript">
                                $(document).ready(function() {
                                    $("#captcha-refresh").click(function () {
                                        $("#captcha").attr("src", "utilities/captcha0a6d.png?rand=" + Math.random());
                                        return false;
                                    });
                                });
                                
                                function contatti_form() {
                                    var ok = true;
                                    var F = eval("document.form_contatti");
                                    
                                    if(ok == true) {
                                        var nome = F.nome.value;
                                        if(nome == ""){
                                            alert("You must enter a value in the NAME field.");
                                            F.nome.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok == true) {
                                        var cognome = F.cognome.value;
                                        if(cognome == ""){
                                            alert("You must enter a value in the SURNAME field.");
                                            F.cognome.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok == true) {
                                        var stringa = F.email.value;
                                        if(((stringa.length-stringa.lastIndexOf('.'))>6)||((stringa.length-stringa.lastIndexOf('.')) < 3)||(stringa.indexOf('@')<0)||(stringa.indexOf('@')!=stringa.lastIndexOf('@'))||((stringa.lastIndexOf('.'))-(stringa.lastIndexOf('@'))<3)) {
                                            alert("You must enter a valid value in the E-MAIL field.");
                                            F.email.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok == true) {
                                        var telefono = F.telefono.value;
                                        if(telefono == ""){
                                            alert("You must enter a value in the TELEPHONE field.");
                                            F.telefono.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok == true) {
                                        var code = F.code.value;
                                        if(code == ""){
                                            alert("You must enter a value in the ANTISPAM field.");
                                            F.code.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok == true) {
                                        var accetto = F.accetto;
                                        if(accetto.checked == false){
                                            alert("To submit the form you must accept the clause concerning the processing of personal data.");
                                            F.accetto.focus();
                                            ok = false;
                                        }
                                    }
                                    
                                    if(ok) {
                                        var url = "utilities/controller_captchad41d.html?";
                                        var pars = "code="+encodeURIComponent($('#code').val());
                                        $.ajax({
                                            type:"GET",
                                            url:url,
                                            data:pars,
                                            success: function(ResponseText) {
                                                var arr_response=ResponseText.split("|");
                                                if(arr_response[0] == "ko") {
                                                    alert("The ANTISPAM field does not contain a valid value.");
                                                }
                                                if(arr_response[0] == "ok") {
                                                    eval("document.form_contatti.submit()");
                                                }
                                            }
                                        });
                                    }
                                }
                                </script> 
